{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd apps/business-dashboard && npm run build",
  "outputDirectory": "apps/business-dashboard/.next",
  "installCommand": "cd apps/business-dashboard && npm install",
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  },
  "functions": {
    "apps/business-dashboard/app/**": {
      "includeFiles": "apps/business-dashboard/**"
    }
  }
}