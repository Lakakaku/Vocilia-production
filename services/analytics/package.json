{
  "name": "@feedback-platform/analytics",
  "version": "1.0.0",
  "description": "Advanced geographic and temporal analytics engine for AI Feedback Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "analytics",
    "geographic",
    "temporal",
    "pattern-detection",
    "fraud-detection",
    "machine-learning"
  ],
  "author": "AI Feedback Platform",
  "license": "MIT",
  "dependencies": {
    "@feedback-platform/shared-types": "workspace:*",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0",
    "mathjs": "^12.2.1",
    "simple-statistics": "^7.8.3",
    "geokdbush": "^4.1.0",
    "ml-kmeans": "^6.0.0",
    "ml-matrix": "^6.10.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}