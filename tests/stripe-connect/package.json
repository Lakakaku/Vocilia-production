{
  "name": "stripe-connect-tests",
  "version": "1.0.0",
  "description": "Comprehensive Stripe Connect Testing for Swedish Market",
  "scripts": {
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "test:onboarding": "jest --config=jest.config.js --testPathPattern=onboarding.test.js",
    "test:webhooks": "jest --config=jest.config.js --testPathPattern=webhooks.test.js",
    "test:payments": "jest --config=jest.config.js --testPathPattern=payments.test.js",
    "test:edge-cases": "jest --config=jest.config.js --testPathPattern=edge-cases.test.js",
    "test:commissions": "jest --config=jest.config.js --testPathPattern=commission.test.js",
    "test:e2e": "jest --config=jest.config.js --testPathPattern=e2e-payment-flow.test.js",
    "setup-webhooks": "node setup-webhook-endpoints.js",
    "validate-test-env": "node validate-test-environment.js",
    "generate-report": "node generate-payment-report.js"
  },
  "dependencies": {
    "stripe": "^14.0.0",
    "jest": "^29.7.0",
    "axios": "^1.5.0",
    "express": "^4.18.0",
    "ngrok": "^5.0.0",
    "crypto": "^1.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/stripe": "^8.0.417",
    "jest-html-reporter": "^3.10.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "setupFilesAfterEnv": ["./test-setup.js"],
    "testMatch": ["**/*.test.js"],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "reporters": [
      "default",
      ["jest-html-reporter", {
        "pageTitle": "Stripe Connect Test Report",
        "outputPath": "./stripe-test-report.html",
        "includeFailureMsg": true
      }]
    ]
  }
}