{
  "dashboard": {
    "id": null,
    "title": "AI Feedback Platform - Comprehensive Performance & Load Testing Dashboard",
    "tags": ["ai-feedback", "performance", "load-testing", "sla", "sweden"],
    "timezone": "Europe/Stockholm",
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"api-gateway\"}",
            "legendFormat": "API Gateway",
            "refId": "A"
          },
          {
            "expr": "up{job=\"business-metrics-exporter\"}",
            "legendFormat": "Metrics Exporter",
            "refId": "B"
          },
          {
            "expr": "up{job=\"postgres-metrics\"}",
            "legendFormat": "Database",
            "refId": "C"
          },
          {
            "expr": "up{job=\"redis-detailed\"}",
            "legendFormat": "Redis Cache",
            "refId": "D"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "unit": "short",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 2,
        "title": "Voice Processing Performance (SLA: <2s)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, ai_feedback_voice_processing_latency_seconds_bucket)",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, ai_feedback_voice_processing_latency_seconds_bucket)",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, ai_feedback_voice_processing_latency_seconds_bucket)",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "max": 5,
            "min": 0
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [2],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["B", "2m", "now"]
              },
              "reducer": {
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "Voice Processing Latency Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "thresholds": [
          {
            "value": 2,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 3,
        "title": "API Response Time (SLA: <500ms)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, ai_feedback_http_request_duration_seconds_bucket{app=\"api-gateway\"})",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, ai_feedback_http_request_duration_seconds_bucket{app=\"api-gateway\"})",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, ai_feedback_http_request_duration_seconds_bucket{app=\"api-gateway\"})",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "max": 2,
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 0.5,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "title": "Load Testing Results - SLA Compliance",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "load_test_sla_compliance{test_type=\"voice-session-100-concurrent\"}",
            "legendFormat": "Voice 100 Concurrent",
            "refId": "A"
          },
          {
            "expr": "load_test_sla_compliance{test_type=\"api-high-throughput-1000rps\"}",
            "legendFormat": "API 1000 RPS",
            "refId": "B"
          },
          {
            "expr": "load_test_sla_compliance{test_type=\"peak-hour-simulation\"}",
            "legendFormat": "Peak Hour",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 5,
        "title": "Peak Capacity Utilization",
        "type": "bargauge",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 16},
        "targets": [
          {
            "expr": "peak_capacity_utilization{test_scenario=\"peak-hour-simulation\"}",
            "legendFormat": "{{ component }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 85
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 6,
        "title": "Swedish Regions - Business Metrics",
        "type": "table",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "sweden_active_businesses_count",
            "legendFormat": "Active Businesses",
            "refId": "A",
            "format": "table"
          },
          {
            "expr": "sweden_daily_feedback_volume",
            "legendFormat": "Daily Feedback",
            "refId": "B",
            "format": "table"
          },
          {
            "expr": "sweden_avg_feedback_quality_score",
            "legendFormat": "Avg Quality",
            "refId": "C",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          }
        ]
      },
      {
        "id": 7,
        "title": "AI Processing Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, ai_feedback_ai_processing_duration_seconds_bucket)",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, ai_feedback_ai_processing_duration_seconds_bucket)",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "ai_feedback_ai_model_accuracy",
            "legendFormat": "Model Accuracy",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Processing Time (seconds) / Accuracy",
            "max": 5,
            "min": 0
          }
        ]
      },
      {
        "id": 8,
        "title": "Payment Processing Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, ai_feedback_payment_processing_duration_seconds_bucket)",
            "legendFormat": "Payment Processing Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, ai_feedback_stripe_api_latency_seconds_bucket)",
            "legendFormat": "Stripe API Latency",
            "refId": "B"
          },
          {
            "expr": "rate(ai_feedback_payment_failures_total[5m]) / rate(ai_feedback_payments_total[5m])",
            "legendFormat": "Payment Failure Rate",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Time (seconds) / Failure Rate",
            "max": 10,
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "Fraud Detection Metrics",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "sweden_fraud_detection_rate",
            "legendFormat": "{{ region }} Detection Rate",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, ai_feedback_fraud_detection_duration_seconds_bucket)",
            "legendFormat": "Processing Time",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 85
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 10,
        "title": "Database Performance",
        "type": "graph",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 30},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, ai_feedback_db_query_duration_seconds_bucket)",
            "legendFormat": "Query Time (95th percentile)",
            "refId": "A"
          },
          {
            "expr": "ai_feedback_db_connections_active",
            "legendFormat": "Active Connections",
            "refId": "B"
          }
        ]
      },
      {
        "id": 11,
        "title": "SLA Compliance Overview",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 30},
        "targets": [
          {
            "expr": "ai_feedback_sla_compliance_percentage{sla_type=\"voice_processing\"}",
            "legendFormat": "Voice SLA",
            "refId": "A"
          },
          {
            "expr": "ai_feedback_sla_compliance_percentage{sla_type=\"api_response\"}",
            "legendFormat": "API SLA",
            "refId": "B"
          },
          {
            "expr": "ai_feedback_availability_uptime_percentage",
            "legendFormat": "Availability",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            },
            "unit": "percent",
            "min": 95,
            "max": 100
          }
        }
      },
      {
        "id": 12,
        "title": "System Resource Utilization",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage - {{ instance }}",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage - {{ instance }}",
            "refId": "B"
          },
          {
            "expr": "(1 - (node_filesystem_avail_bytes / node_filesystem_size_bytes)) * 100",
            "legendFormat": "Disk Usage - {{ instance }}",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Usage Percentage",
            "max": 100,
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 13,
        "title": "Load Testing Detailed Results",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "ai_feedback_load_test_result",
            "legendFormat": "{{ test_type }} - {{ metric }}",
            "refId": "A",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {
                "test_type": 0,
                "metric": 1,
                "scenario": 2,
                "Value": 3
              },
              "renameByName": {
                "test_type": "Test Type",
                "metric": "Metric",
                "scenario": "Scenario",
                "Value": "Result"
              }
            }
          }
        ]
      },
      {
        "id": 14,
        "title": "Business Revenue by Region (SEK)",
        "type": "piechart",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 44},
        "targets": [
          {
            "expr": "sweden_revenue_generated_sek",
            "legendFormat": "{{ region }}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 15,
        "title": "Alert Summary",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 44},
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\",severity=\"critical\"}",
            "legendFormat": "Critical Alerts",
            "refId": "A"
          },
          {
            "expr": "ALERTS{alertstate=\"firing\",severity=\"high\"}",
            "legendFormat": "High Priority",
            "refId": "B"
          },
          {
            "expr": "ALERTS{alertstate=\"firing\",severity=\"warning\"}",
            "legendFormat": "Warnings",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 3
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 16,
        "title": "Recent Performance Events",
        "type": "logs",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 44},
        "targets": [
          {
            "expr": "{job=\"api-gateway\"} |= \"ERROR\" or \"performance\" or \"latency\"",
            "refId": "A"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "name": "Active Alerts",
          "tagKeys": "alertname,severity",
          "titleFormat": "{{ alertname }} ({{ severity }})",
          "textFormat": "{{ annotations.description }}"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(ai_feedback_voice_processing_latency_seconds_bucket, region)",
          "hide": 0,
          "includeAll": true,
          "label": "Region",
          "multi": true,
          "name": "region",
          "options": [],
          "query": {
            "query": "label_values(ai_feedback_voice_processing_latency_seconds_bucket, region)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        },
        {
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(ai_feedback_http_request_duration_seconds_bucket, app)",
          "hide": 0,
          "includeAll": true,
          "label": "Application",
          "multi": true,
          "name": "app",
          "options": [],
          "query": {
            "query": "label_values(ai_feedback_http_request_duration_seconds_bucket, app)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        }
      ]
    }
  },
  "folderId": null,
  "overwrite": true
}