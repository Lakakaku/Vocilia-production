      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "admin-postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  b.name as business_name,\n  b.org_number,\n  bl.region,\n  bl.city,\n  b.tier,\n  b.pilot_status,\n  COUNT(DISTINCT fs.id) as total_sessions,\n  COUNT(DISTINCT CASE WHEN fs.status = 'completed' THEN fs.id END) as completed_sessions,\n  ROUND(AVG(f.quality_score_total), 1) as avg_quality_score,\n  COALESCE(SUM(p.amount), 0) as total_rewards_sek,\n  b.created_at\nFROM businesses b\nLEFT JOIN business_locations bl ON b.id = bl.business_id\nLEFT JOIN feedback_sessions fs ON bl.id = fs.location_id\nLEFT JOIN feedback f ON fs.id = f.session_id\nLEFT JOIN payments p ON f.id = p.feedback_id\nWHERE b.pilot_program = true\nGROUP BY b.id, b.name, b.org_number, bl.region, bl.city, b.tier, b.pilot_status, b.created_at\nORDER BY b.created_at DESC\nLIMIT 20;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Swedish Pilot Businesses - Live Data",
      "type": "table"
