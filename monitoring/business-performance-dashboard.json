{
  "dashboard": {
    "id": null,
    "title": "Business Dashboard Performance Monitor",
    "tags": [
      "business",
      "performance",
      "dashboard"
    ],
    "style": "dark",
    "timezone": "Europe/Stockholm",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Business Dashboard Response Times",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{service=\"business-dashboard\"}[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"business-dashboard\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{service=\"business-dashboard\"}[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 2,
        "title": "Business Dashboard Error Rate",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"business-dashboard\",status=~\"5..\"}[5m]) / rate(http_requests_total{service=\"business-dashboard\"}[5m]) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 15
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 3,
        "title": "Request Rate and Throughput",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"business-dashboard\"}[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          },
          {
            "expr": "business_dashboard_active_sessions",
            "legendFormat": "Active Sessions",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "mappings": []
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 4,
        "title": "Database Performance",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(database_query_duration_seconds_bucket{service=\"business-dashboard\"}[5m]))",
            "legendFormat": "95th percentile query time",
            "refId": "A"
          },
          {
            "expr": "database_connections_active{service=\"business-dashboard\"} / database_connections_max{service=\"business-dashboard\"} * 100",
            "legendFormat": "Connection pool usage %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            },
            "unit": "s"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Connection pool usage %"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                }
              ]
            }
          ]
        }
      },
      {
        "id": 5,
        "title": "Memory and CPU Usage",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes{service=\"business-dashboard\"} / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          },
          {
            "expr": "rate(process_cpu_seconds_total{service=\"business-dashboard\"}[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Memory Usage (MB)"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "decbytes"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "CPU Usage %"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                }
              ]
            }
          ]
        }
      },
      {
        "id": 6,
        "title": "Business-Specific Performance Metrics",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(business_analytics_load_duration_seconds_bucket[5m]))",
            "legendFormat": "Analytics Load Time (95th)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(business_report_generation_duration_seconds_bucket[5m]))",
            "legendFormat": "Report Generation Time (95th)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(business_onboarding_duration_seconds_bucket[5m]))",
            "legendFormat": "Onboarding Time (95th)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            },
            "unit": "s",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Location Performance Breakdown",
        "type": "heatmap",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"business-dashboard\",location_id!=\"\"}[5m])) by (location_id)",
            "legendFormat": "{{ location_id }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Business Tier Performance",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"business-dashboard\",business_tier=\"1\"}[5m]))",
            "legendFormat": "Tier 1 (Basic)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"business-dashboard\",business_tier=\"2\"}[5m]))",
            "legendFormat": "Tier 2 (Professional)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"business-dashboard\",business_tier=\"3\"}[5m]))",
            "legendFormat": "Tier 3 (Premium)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1.5
                },
                {
                  "color": "red",
                  "value": 3
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 9,
        "title": "API Endpoint Performance",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"business-dashboard\",endpoint=~\"/api/.*\"}[5m])) by (endpoint)",
            "format": "table",
            "legendFormat": "",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{service=\"business-dashboard\",endpoint=~\"/api/.*\"}[5m]) by (endpoint)",
            "format": "table",
            "legendFormat": "",
            "refId": "B"
          },
          {
            "expr": "rate(http_requests_total{service=\"business-dashboard\",endpoint=~\"/api/.*\",status=~\"5..\"}[5m]) / rate(http_requests_total{service=\"business-dashboard\",endpoint=~\"/api/.*\"}[5m]) * 100 by (endpoint)",
            "format": "table",
            "legendFormat": "",
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {
              "reducers": []
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Value #A"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "95th Percentile (s)"
                },
                {
                  "id": "unit",
                  "value": "s"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": null
                      },
                      {
                        "color": "yellow",
                        "value": 1
                      },
                      {
                        "color": "red",
                        "value": 2
                      }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Value #B"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Requests/sec"
                },
                {
                  "id": "unit",
                  "value": "reqps"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Value #C"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Error Rate %"
                },
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": null
                      },
                      {
                        "color": "yellow",
                        "value": 5
                      },
                      {
                        "color": "red",
                        "value": 15
                      }
                    ]
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 10,
        "title": "WebSocket Performance",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 48
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(websocket_message_duration_seconds_bucket{service=\"business-dashboard\"}[5m]))",
            "legendFormat": "WebSocket Message Latency (95th)",
            "refId": "A"
          },
          {
            "expr": "websocket_connections_active{service=\"business-dashboard\"}",
            "legendFormat": "Active WebSocket Connections",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "WebSocket Message Latency (95th)"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "s"
                }
              ]
            }
          ]
        }
      },
      {
        "id": 11,
        "title": "Cache Performance",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 48
        },
        "targets": [
          {
            "expr": "rate(cache_hits_total{service=\"business-dashboard\"}[5m]) / (rate(cache_hits_total{service=\"business-dashboard\"}[5m]) + rate(cache_misses_total{service=\"business-dashboard\"}[5m])) * 100",
            "legendFormat": "Cache Hit Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 12,
        "title": "Business Revenue Processing Performance",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 56
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(revenue_calculation_duration_seconds_bucket[5m]))",
            "legendFormat": "Revenue Calculation Time (95th)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(payout_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "Payout Processing Time (95th)",
            "refId": "B"
          },
          {
            "expr": "rate(business_session_timeouts_total[5m])",
            "legendFormat": "Session Timeouts/sec",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10
            },
            "unit": "s"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Session Timeouts/sec"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "ops"
                }
              ]
            }
          ]
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "business_tier",
          "type": "query",
          "query": "label_values(business_tier)",
          "refresh": 1,
          "options": [],
          "includeAll": true,
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "location_id",
          "type": "query",
          "query": "label_values(location_id)",
          "refresh": 1,
          "options": [],
          "includeAll": true,
          "multi": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Performance Alerts",
          "enable": true,
          "iconColor": "red",
          "query": "ALERTS{service=\"business-dashboard\",severity!=\"info\"}",
          "textFormat": "{{ $labels.alertname }}: {{ $labels.summary }}",
          "tags": ["performance", "business-dashboard"],
          "type": "prometheus"
        }
      ]
    }
  }
}