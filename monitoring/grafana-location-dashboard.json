{
  "dashboard": {
    "id": null,
    "title": "Location-Specific Business Monitoring",
    "tags": ["location", "business", "geo-distributed"],
    "style": "dark",
    "timezone": "Europe/Stockholm",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "region",
          "type": "query",
          "query": "label_values(location_health, region)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "multi": true,
          "includeAll": true,
          "options": []
        },
        {
          "name": "business_id",
          "type": "query", 
          "query": "label_values(location_health{region=~\"$region\"}, business_id)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "multi": true,
          "includeAll": true,
          "options": []
        },
        {
          "name": "location_id",
          "type": "query",
          "query": "label_values(location_health{region=~\"$region\",business_id=~\"$business_id\"}, location_id)",
          "current": {
            "text": "All", 
            "value": "$__all"
          },
          "multi": true,
          "includeAll": true,
          "options": []
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Regional Overview",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "count(count by (region)(location_health{region=~\"$region\"}))",
            "legendFormat": "Total Regions",
            "refId": "A"
          },
          {
            "expr": "count(count by (business_id)(location_health{region=~\"$region\",business_id=~\"$business_id\"}))",
            "legendFormat": "Active Businesses",
            "refId": "B"
          },
          {
            "expr": "count(location_health{region=~\"$region\",business_id=~\"$business_id\",location_id=~\"$location_id\"})",
            "legendFormat": "Total Locations",
            "refId": "C"
          },
          {
            "expr": "count(location_health{region=~\"$region\",business_id=~\"$business_id\",location_id=~\"$location_id\",status=\"online\"})",
            "legendFormat": "Online Locations",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "displayName": "${__field.labels.__name__}",
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        }
      },
      {
        "id": 2,
        "title": "Location Health by Region",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "targets": [
          {
            "expr": "count by (region, status)(location_health{region=~\"$region\",business_id=~\"$business_id\",location_id=~\"$location_id\"})",
            "legendFormat": "{{region}} - {{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [
              {
                "options": {
                  "online": {
                    "color": "green",
                    "index": 0
                  },
                  "offline": {
                    "color": "red", 
                    "index": 1
                  },
                  "degraded": {
                    "color": "orange",
                    "index": 2
                  }
                },
                "type": "value"
              }
            ],
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "right"
          }
        }
      },
      {
        "id": 3,
        "title": "Regional Performance Map",
        "type": "geomap",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "targets": [
          {
            "expr": "location_latency_seconds{region=~\"$region\"}",
            "legendFormat": "{{region}} - {{location_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "continuous-GrYlRd"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 0.5
                }
              ]
            },
            "unit": "s"
          }
        },
        "options": {
          "view": {
            "id": "coords",
            "lat": 60,
            "lon": 15,
            "zoom": 5
          },
          "controls": {
            "mouseWheelZoom": true,
            "showAttribution": true,
            "showDebug": false,
            "showScale": false,
            "showZoom": true
          },
          "layers": [
            {
              "config": {
                "style": {
                  "color": {
                    "field": "Value",
                    "fixed": "dark-green"
                  },
                  "opacity": 0.4,
                  "rotation": {
                    "fixed": 0,
                    "max": 360,
                    "min": -360,
                    "mode": "mod"
                  },
                  "size": {
                    "field": "Value",
                    "fixed": 5,
                    "max": 15,
                    "min": 5
                  },
                  "symbol": {
                    "fixed": "img/icons/marker/circle.svg",
                    "mode": "fixed"
                  },
                  "textConfig": {
                    "fontSize": 12,
                    "offsetX": 0,
                    "offsetY": 0,
                    "textAlign": "center",
                    "textBaseline": "middle"
                  }
                }
              },
              "location": {
                "mode": "auto"
              },
              "name": "Location Performance",
              "tooltip": true,
              "type": "markers"
            }
          ]
        }
      },
      {
        "id": 4,
        "title": "QR Code Generation Success Rate by Region",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "rate(qr_generation_success_total{region=~\"$region\"}[5m]) / (rate(qr_generation_success_total{region=~\"$region\"}[5m]) + rate(qr_generation_failures_total{region=~\"$region\"}[5m]))",
            "legendFormat": "{{region}} Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "max": 1,
            "min": 0,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 0.8
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        }
      },
      {
        "id": 5,
        "title": "Customer Engagement by Location",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "targets": [
          {
            "expr": "rate(feedback_sessions_completed_total{region=~\"$region\",business_id=~\"$business_id\",location_id=~\"$location_id\"}[5m]) / rate(feedback_sessions_started_total{region=~\"$region\",business_id=~\"$business_id\",location_id=~\"$location_id\"}[5m])",
            "legendFormat": "{{location_name}} - Completion Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "auto",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "max": 1,
            "min": 0,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 0.3
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "options": {
          "legend": {
            "calcs": ["last"],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi"
          }
        }
      },
      {
        "id": 6,
        "title": "Location Response Times",
        "type": "heatmap",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(location_response_duration_seconds_bucket{region=~\"$region\",location_id=~\"$location_id\"}[5m]))",
            "legendFormat": "{{location_name}} - p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(location_response_duration_seconds_bucket{region=~\"$region\",location_id=~\"$location_id\"}[5m]))",
            "legendFormat": "{{location_name}} - p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(location_response_duration_seconds_bucket{region=~\"$region\",location_id=~\"$location_id\"}[5m]))",
            "legendFormat": "{{location_name}} - p99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "spectrum"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "s"
          }
        },
        "options": {
          "calculate": false,
          "cellGap": 2,
          "cellValues": {},
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "spectrum",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Spectral",
            "steps": 64
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "s"
          }
        }
      },
      {
        "id": 7,
        "title": "Business Location Revenue Impact",
        "type": "bargauge",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        },
        "targets": [
          {
            "expr": "rate(location_revenue_sek_total{region=~\"$region\",business_id=~\"$business_id\",location_id=~\"$location_id\"}[1h])",
            "legendFormat": "{{location_name}} ({{region}})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "continuous-GrYlRd"
            },
            "custom": {
              "displayMode": "gradient",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "SEK/h"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        }
      },
      {
        "id": 8,
        "title": "Alert Status by Location",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        },
        "targets": [
          {
            "expr": "ALERTS{region=~\"$region\",business_id=~\"$business_id\",location_id=~\"$location_id\"}",
            "legendFormat": "",
            "refId": "A",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "green",
                    "index": 0,
                    "text": "OK"
                  },
                  "1": {
                    "color": "red",
                    "index": 1,
                    "text": "FIRING"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {
                "alertname": "Alert",
                "region": "Region",
                "business_id": "Business",
                "location_id": "Location",
                "severity": "Severity",
                "Value": "Status"
              }
            }
          }
        ]
      }
    ]
  }
}