{
  "dashboard": {
    "id": null,
    "title": "SLA Tracking & Reporting Dashboard",
    "tags": ["sla", "performance", "tracking", "reporting", "compliance"],
    "timezone": "Europe/Stockholm",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "SLA Compliance Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "ai_feedback_sla_compliance_percentage{sla_type=\"voice_processing\"}",
            "legendFormat": "Voice Processing SLA",
            "refId": "A"
          },
          {
            "expr": "ai_feedback_sla_compliance_percentage{sla_type=\"api_response\"}",
            "legendFormat": "API Response SLA",
            "refId": "B"
          },
          {
            "expr": "ai_feedback_sla_compliance_percentage{sla_type=\"payment_processing\"}",
            "legendFormat": "Payment Processing SLA",
            "refId": "C"
          },
          {
            "expr": "ai_feedback_availability_uptime_percentage{service=\"overall\"}",
            "legendFormat": "System Availability",
            "refId": "D"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow", 
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            },
            "unit": "percent",
            "min": 95,
            "max": 100,
            "custom": {
              "displayMode": "lcd",
              "orientation": "horizontal"
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Voice Processing SLA Tracking (<2s target)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, ai_feedback_voice_processing_latency_seconds_bucket)",
            "legendFormat": "95th Percentile Latency",
            "refId": "A"
          },
          {
            "expr": "2",
            "legendFormat": "SLA Target (2s)",
            "refId": "B"
          },
          {
            "expr": "(count(rate(ai_feedback_voice_processing_latency_seconds_bucket{le=\"2\"}[5m])) / count(rate(ai_feedback_voice_processing_latency_seconds_bucket{le=\"+Inf\"}[5m]))) * 100",
            "legendFormat": "SLA Compliance %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            },
            "unit": "s",
            "min": 0,
            "max": 5
          }
        }
      },
      {
        "id": 3,
        "title": "API Response Time SLA Tracking (<500ms target)",
        "type": "timeseries", 
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, ai_feedback_http_request_duration_seconds_bucket{app=\"api-gateway\"})",
            "legendFormat": "95th Percentile Response Time",
            "refId": "A"
          },
          {
            "expr": "0.5",
            "legendFormat": "SLA Target (500ms)",
            "refId": "B"
          },
          {
            "expr": "(count(rate(ai_feedback_http_request_duration_seconds_bucket{le=\"0.5\"}[5m])) / count(rate(ai_feedback_http_request_duration_seconds_bucket{le=\"+Inf\"}[5m]))) * 100",
            "legendFormat": "SLA Compliance %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "max": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 0.5
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "SLA Violations by Service (Last 24h)",
        "type": "bargauge",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "increase(ai_feedback_sla_violations_total[24h])",
            "legendFormat": "{{ service }} - {{ violation_type }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 5,
        "title": "Service Availability (99.9% target)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 16},
        "targets": [
          {
            "expr": "ai_feedback_availability_uptime_percentage{service=\"voice-processing\"}",
            "legendFormat": "Voice Processing",
            "refId": "A"
          },
          {
            "expr": "ai_feedback_availability_uptime_percentage{service=\"api-gateway\"}",
            "legendFormat": "API Gateway",
            "refId": "B"
          },
          {
            "expr": "ai_feedback_availability_uptime_percentage{service=\"payments\"}",
            "legendFormat": "Payment System",
            "refId": "C"
          },
          {
            "expr": "ai_feedback_availability_uptime_percentage{service=\"ai-evaluation\"}",
            "legendFormat": "AI Evaluation",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 99
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            },
            "unit": "percent",
            "min": 99,
            "max": 100
          }
        }
      },
      {
        "id": 6,
        "title": "Error Rate SLA Tracking (< 1% target)",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "(rate(ai_feedback_http_requests_total{status_code=~\"5..\"}[5m]) / rate(ai_feedback_http_requests_total[5m])) * 100",
            "legendFormat": "API Error Rate",
            "refId": "A"
          },
          {
            "expr": "(rate(ai_feedback_voice_sessions_failed_total[5m]) / rate(ai_feedback_voice_sessions_total[5m])) * 100",
            "legendFormat": "Voice Session Error Rate", 
            "refId": "B"
          },
          {
            "expr": "1",
            "legendFormat": "SLA Target (1%)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 5,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Load Testing SLA Results",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "load_test_sla_compliance",
            "legendFormat": "{{ test_type }}",
            "refId": "A",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {
                "test_type": 0,
                "target_metric": 1,
                "region": 2,
                "Value": 3
              },
              "renameByName": {
                "test_type": "Test Type",
                "target_metric": "Target Metric",
                "region": "Region",
                "Value": "SLA Compliance (%)"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 8,
        "title": "Monthly SLA Report Summary",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "avg_over_time(ai_feedback_sla_compliance_percentage{sla_type=\"voice_processing\"}[30d])",
            "legendFormat": "Voice Processing (30d avg)",
            "refId": "A"
          },
          {
            "expr": "avg_over_time(ai_feedback_sla_compliance_percentage{sla_type=\"api_response\"}[30d])",
            "legendFormat": "API Response (30d avg)",
            "refId": "B"
          },
          {
            "expr": "avg_over_time(ai_feedback_availability_uptime_percentage[30d])",
            "legendFormat": "System Availability (30d avg)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            },
            "unit": "percent",
            "min": 95,
            "max": 100
          }
        }
      },
      {
        "id": 9,
        "title": "Regional SLA Performance (Sweden)",
        "type": "geomap",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "ai_feedback_sla_compliance_percentage{region=~\"stockholm|gothenburg|malmo\"}",
            "legendFormat": "{{ region }}",
            "refId": "A"
          }
        ],
        "options": {
          "view": {
            "id": "coords",
            "lat": 62.0,
            "lon": 15.0,
            "zoom": 5
          },
          "controls": {
            "mouseWheelZoom": true,
            "showZoom": true,
            "showAttribution": true
          },
          "basemap": {
            "type": "osm-standard"
          }
        }
      },
      {
        "id": 10,
        "title": "SLA Trend Analysis (7 days)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 30},
        "targets": [
          {
            "expr": "ai_feedback_sla_compliance_percentage",
            "legendFormat": "{{ sla_type }}",
            "refId": "A"
          },
          {
            "expr": "99",
            "legendFormat": "Target (99%)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 95,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 99
                }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Critical Alerts Impact on SLA",
        "type": "table",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 30},
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\", severity=\"critical\"}",
            "legendFormat": "{{ alertname }}",
            "refId": "A",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true,
                "alertstate": true
              },
              "renameByName": {
                "alertname": "Alert",
                "severity": "Severity",
                "service": "Service",
                "Value": "Status"
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\", severity=\"critical\"}",
          "iconColor": "red",
          "name": "Critical SLA Violations",
          "tagKeys": "alertname,severity,service",
          "titleFormat": "{{ alertname }}",
          "textFormat": "{{ annotations.description }}"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(ai_feedback_sla_compliance_percentage, sla_type)",
          "hide": 0,
          "includeAll": true,
          "label": "SLA Type",
          "multi": true,
          "name": "sla_type",
          "options": [],
          "query": {
            "query": "label_values(ai_feedback_sla_compliance_percentage, sla_type)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        },
        {
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(ai_feedback_availability_uptime_percentage, service)",
          "hide": 0,
          "includeAll": true,
          "label": "Service",
          "multi": true,
          "name": "service",
          "options": [],
          "query": {
            "query": "label_values(ai_feedback_availability_uptime_percentage, service)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        }
      ]
    }
  },
  "folderId": null,
  "overwrite": true
}