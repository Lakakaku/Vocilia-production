        "id": 1,
        "title": "üè™ Pilot Caf√© Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "Offline", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "Online", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {"steps": [{"color": "red", "value": null}, {"color": "green", "value": 1}]},
            "unit": "none"
          }
        },
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none"
        },
        "targets": [
          {
            "expr": "up{job=~\"pilot-cafe-.*\"}",
            "legendFormat": "{{cafe_name}} - {{cafe_location}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "üìä Today's Feedback Sessions",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {"steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 50}, {"color": "red", "value": 100}]},
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "textMode": "auto",
          "colorMode": "background"
        },
        "targets": [
          {
            "expr": "increase(feedback_sessions_total{pilot_program=\"swedish-cafes\"}[1d])",
            "legendFormat": "Total Sessions",
            "refId": "A"
