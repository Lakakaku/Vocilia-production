{
  "dashboard": {
    "id": null,
    "title": "üìä Business KPIs - Pilot Program",
    "tags": ["business", "kpis", "pilot", "analytics"],
    "style": "dark",
    "timezone": "Europe/Stockholm",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "üí∞ Revenue Impact - Last 24h",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {"steps": [{"color": "green", "value": null}]},
            "unit": "currencySEK"
          }
        },
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area"
        },
        "targets": [
          {
            "expr": "increase(rewards_distributed_sek_total{pilot_program=\"swedish-cafes\"}[24h])",
            "legendFormat": "Rewards Paid",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "üë• Customer Participation Rate",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "green", "value": 15}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 25
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "targets": [
          {
            "expr": "(increase(feedback_sessions_completed_total{pilot_program=\"swedish-cafes\"}[1h]) / increase(qr_scans_total{pilot_program=\"swedish-cafes\"}[1h])) * 100",
            "legendFormat": "Completion Rate",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "‚≠ê Average Quality Score",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 75}
              ]
            },
            "unit": "none",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {"values": false, "calcs": ["mean"]},
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "targets": [
          {
            "expr": "avg(quality_score_average{pilot_program=\"swedish-cafes\"})",
            "legendFormat": "Avg Score",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "‚ö° System Performance Score",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 85},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "targets": [
          {
            "expr": "((1 - rate(http_requests_total{status=~\"5..\",pilot_program=\"swedish-cafes\"}[5m]) / rate(http_requests_total{pilot_program=\"swedish-cafes\"}[5m])) * 100)",
            "legendFormat": "Uptime %",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "üè™ Caf√© Performance Comparison",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {"steps": [{"color": "green", "value": null}]},
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "horizontal",
          "displayMode": "basic",
          "showUnfilled": true
        },
        "targets": [
          {
            "expr": "increase(feedback_sessions_completed_total{pilot_program=\"swedish-cafes\"}[24h])",
            "legendFormat": "{{cafe_name}} - Sessions",
            "refId": "A"
          },
          {
            "expr": "avg_over_time(quality_score_average{pilot_program=\"swedish-cafes\"}[24h])",
            "legendFormat": "{{cafe_name}} - Avg Quality",
            "refId": "B"
          }
        ]
      },
      {
        "id": 6,
        "title": "üí∏ Reward Distribution Breakdown",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "hideFrom": {"tooltip": false, "vis": false, "legend": false}
            },
            "mappings": []
          }
        },
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "pieType": "donut",
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
        },
        "targets": [
          {
            "expr": "increase(rewards_by_tier_sek_total{tier=\"exceptional\",pilot_program=\"swedish-cafes\"}[24h])",
