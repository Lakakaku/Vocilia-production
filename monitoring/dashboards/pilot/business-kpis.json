      {
        "id": 8,
        "title": "üéØ Conversion Funnel Analysis",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "align": "center",
              "displayMode": "color-background"
