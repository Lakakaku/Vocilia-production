          }
        },
        "options": {
          "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom"},
          "tooltip": {"mode": "multi"}
        },
        "targets": [
          {
            "expr": "increase(feedback_sessions_total{pilot_program=\"swedish-cafes\"}[1h])",
            "legendFormat": "Sessions/hour - {{cafe_name}}",
            "refId": "A"
          },
          {
            "expr": "increase(qr_scans_total{pilot_program=\"swedish-cafes\"}[1h])",
