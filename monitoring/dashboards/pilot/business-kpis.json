        },
        "targets": [
          {
            "expr": "increase(qr_scans_total{pilot_program=\"swedish-cafes\"}[1h])",
            "legendFormat": "{{cafe_name}}",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
