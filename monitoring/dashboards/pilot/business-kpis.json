          {
            "expr": "increase(rewards_by_tier_sek_total{tier=\"very_good\",pilot_program=\"swedish-cafes\"}[24h])",
            "legendFormat": "Very Good (75-89)",
            "refId": "B"
          },
          {
            "expr": "increase(rewards_by_tier_sek_total{tier=\"acceptable\",pilot_program=\"swedish-cafes\"}[24h])",
            "legendFormat": "Acceptable (60-74)",
